@model IEnumerable<MediLab.Models.Articulo>

@{
    ViewBag.Title = "BuscarArticulos";
}

<h2>Articulos</h2>

<p>
  
</p>
<table class="table">
    <tr>
        <th></th>
        <th>
            @Html.DisplayNameFor(model => model.Titulo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaPublicacion)
        </th>       
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            <!--<img src="https://upload.wikimedia.org/wikipedia/commons/thumb/a/ac/Approve_icon.svg/620px-Approve_icon.svg.png" style="width:30px;height:30px;" />-->
            <img src="https://cdn2.iconfinder.com/data/icons/flat-seo-web-ikooni/128/flat_seo2-32-512.png" style="width:40px;height:40px;" />
            
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Titulo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaPublicacion)
        </td>      
       <td>
            @Html.ActionLink("Ver mas..", "Edit", new { id=item.Id }, new { @class = "btn btn-primary btn-large" })        
        </td>
    </tr>
}

</table>
